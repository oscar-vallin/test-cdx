{
  "name": "react-ts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@apollo/client": "^3.2.1",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@fluentui/react": "8.52.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "apollo-link-ws": "^1.0.20",
    "apollo-upload-client": "^17.0.0",
    "axios": "^0.21.1",
    "babel-jest": "^27.1.0",
    "chroma-js": "^2.1.1",
    "date-fns": "^2.16.1",
    "easy-peasy": "^5.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "graphql": "^15.3.0",
    "lodash": "^4.17.21",
    "office-ui-fabric-react": "7.181.1",
    "puppeteer": "13.0.1",
    "query-string": "^7.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^3.1.3",
    "react-flow-renderer": "^10.3.7",
    "react-helmet": "^6.1.0",
    "react-meta-tags": "^1.0.1",
    "react-outside-click-handler": "^1.3.0",
    "react-password-strength-bar": "^0.3.3",
    "react-quill": "^1.3.5",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.0",
    "react-toast-component": "^1.4.18",
    "recharts": "2.1.5",
    "styled-components": "5.3.3",
    "subscriptions-transport-ws": "^0.9.18",
    "ts-node": "^10.8.0",
    "typescript": "^4.5.4",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "e2e": "react-scripts test --maxWorkers 1 --testPathPattern=.*e2e.test.ts$",
    "e2e:file": "react-scripts test --maxWorkers 1 --updateSnapshot",
    "test:e2e": "react-scripts test --maxWorkers 1 --testPathPattern=.*e2e.test.ts$ --watchAll=false --reporters=default --reporters=jest-junit ",
    "test:e2e:local": "IS_HEADLESS=\"$npm_config_headless\" && echo \"Is Headless: $IS_HEADLESS\" && export TEST_URL=$npm_config_url && start-server-and-test 'yarn start' http://localhost:3000 'yarn e2e'",
    "test:e2e:url": "REACT_TEMP_URL=\"$npm_config_url\" && echo \"Parsing: $REACT_TEMP_URL\" && npm run e2e",
    "test:e2e:url:file": "REACT_TEMP_URL=\"$npm_config_url\" && echo \"Parsing: $REACT_TEMP_URL\" && npm run e2e:file",
    "test": "react-scripts test --verbose --updateSnapshot --transformIgnorePatterns \"node_modules/(?!react-ts)/\"",
    "test:local": "react-scripts test --verbose --updateSnapshot --transformIgnorePatterns \"node_modules/(?!react-ts)/\" --silent",
    "test:forbuild": "react-scripts --max_old_space_size=4096 test --coverage --watchAll=false --updateSnapshot --reporters=default --reporters=jest-junit --testPathIgnorePatterns \"^.+\\.e2e\\.test\\.(js|ts)\"",
    "test:unit:coverage": "react-scripts test --coverage --watchAll=false --updateSnapshot --reporters=default --reporters=jest-junit --testPathIgnorePatterns \"^.+\\.e2e\\.test\\.(js|ts)\"",
    "test:coverage": "react-scripts test --coverage --runInBand --watchAll=false --updateSnapshot",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\" --color",
    "clean": "rm -rf build",
    "extract-routes": "node --loader ts-node/esm internals/route-extractor.ts",
    "prettify": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --color --fix",
    "isready": "yarn run format && yarn run lint && yarn run test:coverage && yarn run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.12.5",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "chalk": "^4.1.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-only-warn": "npm:@aminya/eslint-plugin-only-warn@^1.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "^4.3.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^12.2.0",
    "lint-staged": "^10.4.0",
    "postcss-normalize": "^10.0.1",
    "prettier": "^2.1.2",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^27.0.5",
    "yarn-or-npm": "^3.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json}": [
      "yarn lint"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/reportWebVitals.ts"
    ],
    "coveragePathIgnorePatterns": [
      "src/.*\\.*types\\.{ts,tsx}",
      "src/index\\.tsx",
      "src/serviceWorker\\.ts",
      "src/main/resources/static/.*",
      "/\\.gradle",
      "/\\.node/",
      "/\\.scannerwork/",
      "/build/",
      "/node/",
      "/node_modules/",
      "src/pages",
      "src/utils",
      "src/store",
      "src/contexts",
      "src/data",
      "src/hooks",
      "src/teste2e",
      "src/use-cases"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 9,
        "branches": 7,
        "lines": 9,
        "functions": 6
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "office-ui-fabric-react/lib/(.*)$": "office-ui-fabric-react/lib-commonjs/$1",
      "src/pages/(.*)$": "<rootDir>/src/pages/$1"
    }
  },
  "jest-junit": {
    "outputDirectory": "test-results",
    "outputName": "junit.xml"
  }
}
