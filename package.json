{
  "name": "react-ts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@apollo/client": "^3.2.1",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@fluentui/react": "^7.144.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.3",
    "@uifabric/react-cards": "^0.115.13",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "apollo-link-ws": "^1.0.20",
    "axios": "^0.21.1",
    "babel-jest": "^27.1.0",
    "chroma-js": "^2.1.1",
    "date-fns": "^2.16.1",
    "easy-peasy": "^5.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "graphql": "^15.3.0",
    "lodash": "^4.17.21",
    "office-ui-fabric-react": "^7.144.2",
    "password-validator": "^5.1.1",
    "puppeteer": "^10.2.0",
    "query-string": "^7.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^3.1.3",
    "react-helmet": "^6.1.0",
    "react-password-strength-bar": "^0.3.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "react-toast-component": "^1.4.18",
    "recharts": "^2.0.6",
    "styled-components": "^5.2.0",
    "subscriptions-transport-ws": "^0.9.18",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "zxcvbn": "^4.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose --updateSnapshot --transformIgnorePatterns \"node_modules/(?!react-ts)/\"",
    "test:local": "react-scripts test --verbose --updateSnapshot --transformIgnorePatterns \"node_modules/(?!react-ts)/\" --silent",
    "test:forbuild": "react-scripts test --coverage --watchAll=false --updateSnapshot --reporters=default --reporters=jest-junit --testPathIgnorePatterns \"^.+\\.e2e\\.test\\.(js|ts)\"",
    "test:coverage": "react-scripts test --coverage --runInBand --watchAll=false",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "yarn lint:fix",
    "eject": "react-scripts eject",
    "clean": "rm -rf build",
    "extract-routes": "node --experimental-modules internals/route-extractor.mjs",
    "prettify": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --color --fix",
    "isready": "yarn run format && yarn run lint && yarn run test:coverage && yarn run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.12.5",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "chalk": "^4.1.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-only-warn": "npm:@aminya/eslint-plugin-only-warn",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^12.2.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "ts-jest": "^27.0.5",
    "yarn-or-npm": "^3.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json}": [
      "yarn lint"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/reportWebVitals.ts"
    ],
    "coveragePathIgnorePatterns": [
      "src/.*\\.*types\\.{ts,tsx}",
      "src/index\\.tsx",
      "src/serviceWorker\\.ts",
	  "src/main/resources/static/.*",
	  "/\\.gradle",
	  "/\\.node/",
	  "/\\.scannerwork/",
	  "/build/",
	  "/node/",
	  "/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 9,
        "branches": 7,
        "lines": 9,
        "functions": 6
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "src/pages/(.*)$": "<rootDir>/src/pages/$1"
    }
  },
  "jest-junit": {
    "outputDirectory": "test-results",
    "outputName": "junit.xml"
  }
}
